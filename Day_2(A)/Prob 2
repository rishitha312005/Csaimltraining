#include <stdio.h>

int main() {
    int n, totalTax = 0;
    char vehicleType[10];

    printf("Enter number of vehicles: ");
    scanf("%d", &n);

    for(int i = 0; i < n; i++) {
        printf("Enter vehicle type (Car, Truck, Bike, Bus): ");
        scanf("%s", vehicleType);

        switch(vehicleType[0]) {
            case 'C':
                totalTax += 50;
                break;
            case 'T':
                totalTax += 100;
                break;
            case 'B':
                if(vehicleType[1] == 'i')
                    totalTax += 20; // Bike
                else
                    totalTax += 80; // Bus
                break;
            default:
                printf("Unknown vehicle type\n");
                break;
        }
    }

    printf("Total Toll Tax: â‚¹%d\n", totalTax);
    return 0;
}
