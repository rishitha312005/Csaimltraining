#include<stdio.h>
void main()
{
    int i,n,a[10],max;
    printf("enter the number of elements:");
    
    scanf("%d",&n);
    printf("the array elements are");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    max=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
        
    }
    printf("largest number is %d",max);
}
